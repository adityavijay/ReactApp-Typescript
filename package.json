{
  "name": "Islington",
  "version": "0.1.0",
  "author": "Aditya vijay",
  "private": true,
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": ["css-modules-transform"]
  },
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack",
    "buildserver": "babel ./server/*.js -d build -w",
    "buildclient": "babel ./shared/**/*.js -d build -w",
    "runserver": "node ./build/server/app.js",
    "watch": "webpack --progress --watch",
    "buildapp": "babel ./server/*.js ./shared/**/*.js -d build -w",
    "nodemon": "nodemon ./build/server/app.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "csv-loader": "^2.1.1",
    "express": "^4.16.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "matchdep": "*",
    "nodemon": "^1.14.11",
    "prop-types": "^15.6.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "^1.3.1",
    "webpack-utf8-bom": "^1.1.3",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.2.0"
  }
}
